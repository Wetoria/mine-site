{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "preserve",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
    }
  },
  "include": [
    "./**/*.ts",
    "./**/*.d.ts",
    ".**/*.vue",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
